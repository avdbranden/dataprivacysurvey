<div id="rapport-title">
  <div class="container text-center">
    <div class="row">
      <div class="col-xs-12 col-md-10 col-md-offset-1">
        <h1>Compliance Rapport</h1>
      </div> <!-- .cols-xs-12 col-md-10 col-md-offset-1 -->
    </div> <!-- .row -->
  </div> <!-- .container -->
</div> <!-- #rapport-title -->

<div id="rapport-resume">
  <div class="container text-center">
    <div class="row">
      <p>The total is for directive is <%= @score_directive %></p>
      <p>The total is for directive is <%= @score_regulation %></p>
    </div> <!-- .row -->
  </div> <!-- .container -->
</div> <!-- #rapport-resume -->

<div id="rapport-answers">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <% @survey.answers.all.each do |answer| %>
          <p><%= answer.question_text %></p>
        <% end%>
      </div> <!-- .col-xs-12 -->
    </div> <!-- .row -->
  </div> <!-- .container -->
</div> <!-- #rapport-answers -->
