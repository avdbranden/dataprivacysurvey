<!-- Title Start -->

<div id="rapport-title">
  <div class="container text-center">
    <div class="row">
      <div class="col-xs-12 col-md-10 col-md-offset-1">
        <h1>Compliance Report</h1>
      </div> <!-- .cols-xs-12 col-md-10 col-md-offset-1 -->
    </div> <!-- .row -->
  </div> <!-- .container -->
</div> <!-- #rapport-title -->

<!-- Title End -->

<!-- Resume Start -->

<div id="rapport-resume">
  <div class="container text-center">
    <div class="row">
      <div class="col-xs-12 col-md-4 col-md-offset-1">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h2>Directive 95/46</h2>
          </div> <!-- .panel-heading -->
          <div class="panel-body">
            <h3>Compliance level</h3>
            <div class="compliance-result">
              <h3 id="dir_percentage"><%= number_with_precision(@compliance_directive_percentage, precision: 2, strip_insignificant_zeros: true) %>%</h3>
            </div>
            <div class="compliance-bar">
              <div class="compliance-bar-progress compliance-bar-dir">
              </div>
            </div> <!-- .compliance-bar -->
          </div> <!-- .panel-body -->
        </div> <!-- .panel -->
      </div> <!-- .cols-xs-12 col-md-10 col-md-offset-1 -->

      <div class="col-xs-12 col-md-4 col-md-offset-2">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h2>Regulation 679</h2>
          </div> <!-- .panel-heading -->
          <div class="panel-body">
            <h3>Compliance level</h3>
            <div class="compliance-result">
              <h3 id="reg_percentage"><%= number_with_precision(@compliance_regulation_percentage, precision: 2, strip_insignificant_zeros: true) %>%</h3>
            </div>
            <div class="compliance-bar">
              <div class="compliance-bar-progress compliance-bar-reg">
              </div>
            </div> <!-- .compliance-bar -->
          </div> <!-- .panel-body -->
        </div> <!-- .panel -->
      </div> <!-- .cols-xs-12 col-md-10 col-md-offset-1 -->

    </div> <!-- .row -->
  </div> <!-- .container -->
</div> <!-- #rapport-resume -->

<!-- Resume End -->

<!-- Thank you Start -->

<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-6 col-md-offset-3">
      <div class="text-center" id="thank-you-box">
        <div class="panel">
          <div class="panel-heading">
            <h1>Thank you very much for filling out the survey!</h1>
          </div>
          <div class="panel-body">
            <h3>If you want be in touch please contact us at <strong>sample@test.com</strong></h3>
            <p>Please note that the compliance levels above are for indicative purposes only. The answers to the questions are anonymized and will be used for statistical purposes only.</p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div> <!-- #rapport-resume -->

<!-- Thank you End -->
